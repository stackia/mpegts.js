{
  "name": "mpegts.js",
  "version": "1.8.0",
  "description": "HTML5 MPEG2-TS Stream Player",
  "main": "./dist/mpegts.esm.js",
  "module": "./dist/mpegts.esm.js",
  "types": "./d.ts/mpegts.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mpegts.esm.js",
      "types": "./d.ts/mpegts.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xqq/mpegts.js"
  },
  "keywords": [
    "html5",
    "mpegts",
    "m2ts",
    "mse",
    "arib",
    "dvb"
  ],
  "scripts": {
    "build": "vite build --mode=production",
    "build:debug": "vite build --mode=development",
    "dev": "vite",
    "preview": "vite preview",
    "format": "prettier --write ."
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/express-serve-static-core": "4.17.37",
    "@types/node": "^24.9.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-node-polyfills": "^0.24.0"
  },
  "author": "zheng qian <xqq@xqq.im>",
  "license": "Apache-2.0"
}
